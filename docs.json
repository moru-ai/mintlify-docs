{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Moru",
  "colors": {
    "primary": "#954535",
    "light": "#B5655A",
    "dark": "#733628"
  },
  "favicon": "/favicon.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": ["index", "quickstart"]
          },
          {
            "group": "Sandbox",
            "pages": [
              "sandbox/overview",
              "sandbox/create",
              "sandbox/connect",
              "sandbox/kill",
              "sandbox/timeout"
            ]
          },
          {
            "group": "Files",
            "pages": [
              "files/overview",
              "files/read",
              "files/write",
              "files/list",
              "files/manage",
              "files/watch"
            ]
          },
          {
            "group": "Commands",
            "pages": [
              "commands/overview",
              "commands/run",
              "commands/background",
              "commands/pty",
              "commands/stdio"
            ]
          },
          {
            "group": "Monitoring",
            "pages": ["monitoring/logs", "monitoring/metrics"]
          },
          {
            "group": "Templates",
            "pages": [
              "templates/overview",
              "templates/build",
              "templates/dockerfile",
              "templates/registries",
              "templates/dashboard"
            ]
          },
          {
            "group": "CLI",
            "pages": [
              "cli/install",
              "cli/auth",
              "cli/sandbox",
              "cli/template"
            ]
          },
          {
            "group": "SDKs",
            "pages": ["sdk/python", "sdk/javascript"]
          },
          {
            "group": "Configuration",
            "pages": ["configuration/environment"]
          },
          {
            "group": "Errors",
            "pages": ["errors"]
          },
          {
            "group": "Limits & Pricing",
            "pages": ["limits/rate-limits", "limits/quotas", "pricing"]
          },
          {
            "group": "Examples",
            "pages": ["examples/maru"]
          }
        ]
      },
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Claude Agent SDK",
            "pages": [
              "guides/claude-agent-sdk/overview",
              "guides/claude-agent-sdk/setup",
              "guides/claude-agent-sdk/session-persistence",
              "guides/claude-agent-sdk/schemas",
              "guides/claude-agent-sdk/example"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Dashboard",
          "href": "https://moru.io/dashboard",
          "icon": "gauge"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/moru-ai",
          "icon": "github"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@moru.io"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://moru.io/dashboard"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/moru_ai",
      "github": "https://github.com/moru-ai"
    }
  }
